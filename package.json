{
  "name": "rps-ethereum",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean:install": "rimraf node_modules && yarn install",
    "clean:contracts": "rimraf contractsBuild && truffle compile",
    "serve": "vue-cli-service serve",
    "build:dapp": "vue-cli-service build",
    "build:contracts": "truffle compile",
    "lint:js": "vue-cli-service lint",
    "lint:sol": "solium -d ./contracts",
    "test:e2e": "vue-cli-service test:e2e",
    "test:dapp": "vue-cli-service test:unit",
    "start:blockchain": "ganache-cli -g 5e9 -l 6500000 -d -m 'resource leg moment usage square town husband gain diary bitter muscle diagram'",
    "start:blockchain:delayed": "ganache-cli -b 15 -g 5e9 -l 6500000 -d -m 'resource leg moment usage square town husband gain diary bitter muscle diagram'",
    "test:contracts": "truffle test --reset",
    "test:contracts:gas-reporter": "GAS_REPORTER=true yarn run test:contracts",
    "migrate:dev": "truffle migrate --reset --network dev",
    "migrate:kovan": "truffle migrate --reset --network kovan",
    "migrate:rinkeby": "truffle migrate --reset --network rinkeby",
    "migrate:ropsten": "truffle migrate --reset --network ropsten",
    "migrate:mainnet": "truffle migrate --reset --network live",
    "exec:local": "truffle exec --network dev",
    "deploy": "./deploy.sh"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@mdi/font": "^2.8.94",
    "crypto-js": "^3.1.9-1",
    "ganache-cli": "^6.2.3",
    "ramda": "^0.25.0",
    "rimraf": "^2.6.2",
    "truffle": "^5.0.0-beta.2",
    "vue": "^2.5.17",
    "vue-clipboard2": "^0.2.1",
    "vue-router": "^3.0.1",
    "vue-typer": "^1.2.0",
    "vuetify": "^1.2.0",
    "vuex": "^3.0.1",
    "vuex-persist": "^2.0.0",
    "vuex-router-sync": "^5.0.0",
    "web3": "^1.0.0-beta.37",
    "web3-provider-engine": "^14.1.0",
    "web3-utils": "^1.0.0-beta.37"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^3.0.0",
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-plugin-e2e-cypress": "^3.0.4",
    "@vue/cli-plugin-eslint": "^3.0.4",
    "@vue/cli-plugin-unit-jest": "^3.0.4",
    "@vue/cli-service": "^3.0.4",
    "@vue/eslint-config-prettier": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-imports": "^1.5.1",
    "bn.js": "^4.11.8",
    "chalk": "^2.4.1",
    "dotenv": "^6.1.0",
    "eslint": "^5.8.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-compat": "^2.6.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "eth-gas-reporter": "^0.1.12",
    "material-design-icons-iconfont": "^4.0.2",
    "openzeppelin-solidity": "^2.0.0",
    "solc": "0.4.25",
    "solium": "^1.1.8",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "truffle-hdwallet-provider": "^0.0.6",
    "vue-template-compiler": "^2.5.17",
    "vuetify-loader": "^1.0.8"
  }
}
